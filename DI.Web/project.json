{
    "version": "1.0.0-rc1",
    "webroot": "wwwroot",
    "exclude": [
        "wwwroot"
    ],
    "packExclude": [
        "**.kproj",
        "**.user",
        "**.vspscc"
    ],
    "dependencies": {
        "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-16064",
        "Microsoft.AspNet.Diagnostics": "1.0.0-rc1-16182",
        "Microsoft.AspNet.Mvc": "6.0.0-rc1-16441",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-rc1-15963"
        
    },
    "commands": {
        "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5001",
        "kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://*:5004"
    },
    "frameworks": {
         "dnx451": { },
         "dnxcore50": { }
    }
}